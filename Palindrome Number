#include<iostream>
using namespace std;

int main(){
    int num,r,sum=0;
    cout<<"enter the number to check: ";
    cin>>num;
    int temp = num;
    while(num!=0)
    {
        r = num%10;
        sum = (sum*10)+r;
        num = num/10;
    }
    if(temp==sum){
        cout<<"Pallindrome";
    }
    else cout<<"Not Pallindrome";
    return 0;
}
